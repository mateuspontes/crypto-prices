{
  "name": "crypto-prices",
  "version": "1.0.0",
  "description": "Get price of the most popular crypto coins on CoinMarketCap",
  "main": "main.js",
  "scripts": {
    "clear": "rimraf bin",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir bin src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint"
  },
  "preferGlobal": true,
  "bin": {
    "crypto-prices": "bin/main.js"
  },
  "keywords": ["crypto", "coins", "marketcap", "price"],
  "author": "Mateus Pontes <mateuspo10@gmail.com> (https://mateuspontes.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "commander": "^2.10.0",
    "ora": "^1.3.0",
    "request": "^2.81.0"
  }
}
